    				</div>
    			  </div>
				  </form>
    			</div>
    			#* END IMPORT PPT *#			
		</div>
		<script>
			var jwpAddVideoSlidePreview = null;
			
			function disbaledRemoveFromSaveButtonOnUploadVideoSection(uploader,submitSelector){
				var submitBtn = $(uploader).closest('form').find(submitSelector);
				submitBtn.removeAttr('disabled');
				submitBtn.addClass('blue');
				submitBtn.removeClass('btn-default');
				
			}
						
			function onVideoUploadDone(uploader,submitSelector) {
				enableAssetSubmission(uploader,submitSelector);
				$(uploader).find('#btnRemoveVideo').removeAttr('disabled');
				$(uploader).find("#addVideoSlidPreview").removeClass("hide");
				$(uploader).find('#dropPreview').addClass('hide');
				if(jwplayer) {
					jwpAddVideoSlidePreview = jwplayer($(uploader).find("#addVideoSlidPreview")[0]);
					var folderPath = "$!{lcmsProperties.getLCMSProperty('lcms.preview.streaming')}";
					jwpAddVideoSlidePreview.setup({
						file: folderPath + $(uploader).find("#filePath").val(),
						width :"100%"
					});
				}
				
			}
			
			function onVideoDraggingActive(target) {
				var uploader = $(target).closest(".upload-360")[0];
				if($(uploader).find("#addVideoSlidPreview").is(':visible')) {
					$(uploader).find('#dropPreview').removeClass('hide');	
				}
			}
			
			function onVideoDraggingInactive(target) {
				var uploader = $(target).closest(".upload-360")[0];
				if($(uploader).find("#addVideoSlidPreview").is(':visible')) {
					$(uploader).find('#dropPreview').addClass('hide');	
				}
			}
			
			function removeTempVideo(target,noConfirmation) {
				var action = function(){
					var uploader = $(target).closest(".upload-360")[0];
					$(uploader).uploader360().reset();
				};
				
				if(noConfirmation) {
					action();
					return;
				}
				
				var trgModal = $("#confirmationModal");
				var title = '<i class="icon-exclamation-sign"></i> Please Confirm';
				var msg = '<p>This action will remove the Streaming asset from this template. Do you want to continue?</p>';		
				var btns = '<button id="btnYes" type="button" class="btn blue" data-dismiss="modal">YES</button>'+
				'<button type="button" class="btn btn-default"  data-dismiss="modal">NO</button>';
				
				trgModal.find(".modal-title").html(title);
				trgModal.find(".modal-body").html(msg);
				trgModal.find(".modal-footer").html(btns);
				trgModal.modal('show');
				trgModal.find('#btnYes').click(action);
	
			}
			
			function removeExcel(target,noConfirmation) {
				var action = function(){
					var uploader = $(target).closest(".upload-360")[0];
					$(uploader).uploader360().reset();
				};
				
				if(noConfirmation) {
					action();
					return;
				}
				
				var trgModal = $("#confirmationModal");
				var title = '<i class="icon-exclamation-sign"></i> Please Confirm';
				var msg = '<p>This action will remove excel file from this template. Do you want to continue?</p>';		
				var btns = '<button id="btnYes" type="button" class="btn blue" data-dismiss="modal">YES</button>'+
				'<button type="button" class="btn btn-default"  data-dismiss="modal">NO</button>';
				
				trgModal.find(".modal-title").html(title);
				trgModal.find(".modal-body").html(msg);
				trgModal.find(".modal-footer").html(btns);
				trgModal.modal('show');
				trgModal.find('#btnYes').click(action);
				$(target).find('#dropPreview').addClass('hide');
	
			}
			function enableAssetSubmission(uploader,submitSelector) {
				
				
				var submitBtn = $(uploader).closest('form').find(submitSelector);
				submitBtn.removeAttr('disabled');
				submitBtn.addClass('blue');
				submitBtn.removeClass('btn-default');
				$(uploader).find('#btnRemoveVideo').removeAttr('disabled');
				$(uploader).closest('form').find("#dropPreview").addClass('hide');
				
			}
			
			function enableExcelFileSubmission(uploader,submitSelector) {
				
				var submitBtn = $(uploader).closest('form').find(submitSelector);
				submitBtn.removeAttr('disabled');
				submitBtn.addClass('blue');
				submitBtn.removeClass('btn-default');
				$(uploader).find('#btnRemoveVideo').removeAttr('disabled');
				$(uploader).find('.drop-area').addClass('hide');
			}

			function disableAssetSubmission(uploader,submitSelector) {
				
				$(uploader).find('#btnRemoveVideo').attr('disabled','true');
				$(uploader).find('#dropPreview').removeClass('hide');
				$(uploader).find("#addVideoSlidPreview").addClass("hide");
				if(jwpAddVideoSlidePreview) {
					jwpAddVideoSlidePreview.stop();
				}
							
				addDisabledOnSaveButton(uploader,submitSelector);
				
			}
			function addDisabledOnSaveButton(uploader,submitSelector){
					var submitBtn = $(uploader).closest('form').find(submitSelector);
					submitBtn.attr('disabled','true');
					submitBtn.removeClass('blue');
					submitBtn.addClass('btn-default');
			
			}
		</script>
		#* END MODALS *#